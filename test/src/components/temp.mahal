<script>
import {Component, Computed, Reactive} from "mahal";
import {State,Mutation,Expression } from "mahal-store";

export default class extends Component {

    @State('count')
    counter 

    @Expression('countPlus5') countPlus5;

    
    increment() {
        (this as any).store.set('incrementCount');
        console.log("triggered increment", 
        (this as any).store.get('count')
        );
    }

    @Mutation('decrementCount')
    decrement ;
}
</script>

 